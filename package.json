{
  "name": "jungle-book",
  "version": "0.1.0",
  "description": "Sample code and documentation for the Tableau Extensions API.",
  "scripts": {
    "start": "node node_modules/http-server/bin/http-server -p 8457",
    "lint": "semistandard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LGraber/trex-jungle-book.git"
  },
  "author": "lgraber",
  "license": "MIT",
  "dependencies": {
    "http-server": "^0.9.0"
  },
  "devDependencies": {
    "semistandard": "^11.0.0"
  },
  "semistandard": {
    "env": {
      "jquery": true,
      "browser": true,
      "tableau": true
    },
    "globals": [
      "tableau"
    ],
    "rules": {
        "no-unused-vars": "off"
    },
    "ignore": [
      "/lib",
      "/docs",
      "tableau-*.js"
    ]
  }
}
